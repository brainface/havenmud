xt spawn.%^RESET%^\"",}),({1316386530,"%^BOLD%^RED%^You tell A presence,%^RESET%^ \"Great...%^RESET%^\"",}),({1316386544,"%^BOLD%^RED%^You tell A presence,%^RESET%^ \"They already take 3x longer to kill :(.%^RESET%^\"",}),({1316386573,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"That's just them getting slash combat while they have a sword.%^RESET%^\"",}),({1316387373,"%^BOLD%^RED%^You tell A presence,%^RESET%^ \"...he seems to keep moving.%^RESET%^\"",}),({1316387377,"%^BOLD%^RED%^You tell A presence,%^RESET%^ \"When we're being attacked.%^RESET%^\"",}),({1316387391,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"He does. i'm just removing offensive spells.%^RESET%^\"",}),({1316388354,"%^BOLD%^RED%^You tell A presence,%^RESET%^ \"Did you update the runner to be unique?%^RESET%^\"",}),})
