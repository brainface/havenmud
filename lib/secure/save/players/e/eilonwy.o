tellhist ({({1501042060,"%^BOLD%^RED%^You tell Mahkefel,%^RESET%^ \"Thanks.%^RESET%^\"",}),({1501043251,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"So like... one of the things we did when we updated priest/cleric was give kylin evoko and make the blasty kylin spells no longer enchantment.%^RESET%^\"",}),({1501043281,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"But hey! you get all healing/enchant/evoko now and all the other blasty spells.%^RESET%^\"",}),({1501043295,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"(also mithril working, but you got that already).%^RESET%^\"",}),({1501043296,"%^BOLD%^RED%^You tell Mahkefel,%^RESET%^ \"She'll use it for good.%^RESET%^\"",}),({1501043364,"%^BOLD%^RED%^You tell Mahkefel,%^RESET%^ \"Looking at my spells really the only thing that's \"blasty\" is the Kylin stuff anyway, everything else is utility, and I have no shields worth a damn at the moment.%^RESET%^\"",}),({1501043376,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"Yeah it'll take you a bit.%^RESET%^\"",}),({1501043391,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"To train back up.%^RESET%^\"",}),({1501043406,"%^BOLD%^RED%^You tell Mahkefel,%^RESET%^ \"81 points so far.%^RESET%^\"",}),({1501043423,"%^BOLD%^RED%^You tell Mahkefel,%^RESET%^ \"Starting to actually cost points though.%^RESET%^\"",}),({1501043433,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"Yay.%^RESET%^\"",}),({1501043444,"%^BOLD%^RED%^You tell Mahkefel,%^RESET%^ \"82 == 20500.%^RESET%^\"",}),({1501043475,"%^BOLD%^RED%^You tell Mahkefel,%^RESET%^ \"Hey do me a favor, throw a bell at me.%^RESET%^\"",}),({1501043484,"%^BOLD%^RED%^You tell Mahkefel,%^RESET%^ \"Nope...%^RESET%^\"",}),({1501043494,"%^BOLD%^RED%^You tell Mahkefel,%^RESET%^ \"No audible report.%^RESET%^\"",}),({1501043708,"%^BOLD%^RED%^You tell Mahkefel,%^RESET%^ \"One more time, a bell if you don't mind.%^RESET%^\"",}),({1501043724,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"Can you not bell yourself?%^RESET%^\"",}),({1501043744,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"(also fwiw, newbie cleric trainers in westwood updated to give bows).%^RESET%^\"",}),({1501043744,"%^BOLD%^RED%^You tell Mahkefel,%^RESET%^ \"Sweet, now it rings a bell.%^RESET%^\"",}),({1501043758,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"Er, in soleil.%^RESET%^\"",}),({1501043763,"%^BOLD%^RED%^You tell Mahkefel,%^RESET%^ \"OooOoOoo, that's cool.%^RESET%^\"",}),({1501043765,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"*priest trainers.%^RESET%^\"",}),({1501043790,"%^BOLD%^RED%^You tell Mahkefel,%^RESET%^ \"I'm interested to see what they do now, might reroll Relg.%^RESET%^\"",}),({1501043812,"%^BOLD%^RED%^You tell Mahkefel,%^RESET%^ \"Can you still suicide a character?%^RESET%^\"",}),({1501043815,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"Yarr.%^RESET%^\"",}),({1501043823,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"But the only change is a free bow.%^RESET%^\"",}),({1501043832,"%^BOLD%^RED%^You tell Mahkefel,%^RESET%^ \"No skillset change?%^RESET%^\"",}),({1501043839,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"How old is relg?%^RESET%^\"",}),({1501043841,"%^BOLD%^RED%^You tell Mahkefel,%^RESET%^ \"Minutes.%^RESET%^\"",}),({1501043847,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"Yeah, same skills.%^RESET%^\"",}),({1501043864,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"Eclat didn't get a lot of punch out of the skillset change, just got natural magic and dodge.%^RESET%^\"",}),({1501043864,"%^BOLD%^RED%^You tell Mahkefel,%^RESET%^ \"So ranger is still the way to do Muezzins for now.%^RESET%^\"",}),({1501043879,"%^BOLD%^RED%^You tell Mahkefel,%^RESET%^ \"Nat magic helps tremendously.%^RESET%^\"",}),({1501043890,"%^BOLD%^RED%^You tell Mahkefel,%^RESET%^ \"Want a real clusterfuck of skills look at Redhaven's.%^RESET%^\"",}),({1534193546,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"You thar?%^RESET%^\"",}),({1534193811,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"Bah!%^RESET%^\"",}),({1534198765,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"You see the extra info on score/armours?%^RESET%^\"",}),({1534198810,"%^BOLD%^RED%^You tell Mahkefel,%^RESET%^ \"Fuck yeah, redid my gear over it.%^RESET%^\"",}),({1534198875,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"Sweet.%^RESET%^\"",}),({1534198901,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"Trying to make things a little more visible, so we can at least know what is/isn't good/hilariously bad.%^RESET%^\"",}),})
