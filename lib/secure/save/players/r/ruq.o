tellhist ({({1408940134,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"One question. would he call those other races the noble races.%^RESET%^\"",}),({1408940219,"%^BOLD%^RED%^You tell Mahkefel,%^RESET%^ \"Ah, no he wouldn't.%^RESET%^\"",}),({1408940252,"%^BOLD%^RED%^You tell Mahkefel,%^RESET%^ \"What would be the better word?%^RESET%^\"",}),({1408940281,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"Fair maybe.%^RESET%^\"",}),({1408940313,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"Errr highborn.%^RESET%^\"",}),({1408940314,"%^BOLD%^RED%^You tell Mahkefel,%^RESET%^ \"Humm, that might imply that they could be just.  How about pretty.%^RESET%^\"",}),({1408940321,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"Aristocratic.%^RESET%^\"",}),({1408940364,"%^BOLD%^RED%^You tell Mahkefel,%^RESET%^ \"Aristocratic could work.%^RESET%^\"",}),({1408940372,"%^BOLD%^RED%^You tell Mahkefel,%^RESET%^ \"What are you doing up so late anyway?%^RESET%^\"",}),({1408940377,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"If you're good with that I'll approve (the original is fine, that one word just seemed like not what an orc would say).%^RESET%^\"",}),({1408940384,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"Making poor life choices, duh.%^RESET%^\"",}),({1408940399,"%^BOLD%^RED%^You tell Mahkefel,%^RESET%^ \"Hahaha.  I am fine with that.%^RESET%^\"",}),({1408940437,"%^BOLD%^RED%^You tell Mahkefel,%^RESET%^ \"Thanks.%^RESET%^\"",}),})
