tellhist ({({1501514944,"%^BOLD%^RED%^You tell Mahkefel,%^RESET%^ \"Or... fudges your level vs the fear level, so you are not affected by it at a lower level.%^RESET%^\"",}),({1501514946,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"Yeah it used to make you immune looks like.%^RESET%^\"",}),({1501514948,"%^BOLD%^RED%^You tell Mahkefel,%^RESET%^ \"Something like that.%^RESET%^\"",}),({1501514967,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"If... your level * your bless amount is greater than 2* the fear level you pass, looks like?%^RESET%^\"",}),({1501514978,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"That's... probably actually effectively fear immunity.%^RESET%^\"",}),({1501515004,"%^BOLD%^RED%^You tell Mahkefel,%^RESET%^ \"Bless amount? like, the current amount ? or the initial amount?%^RESET%^\"",}),({1501515036,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"If ((who->GetLevel() * who->GetProperty(\"blessed\") ? 2: 1)) > lvl).%^RESET%^\"",}),({1501515166,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"This is where i play the \"is that code even functioning?\" game.%^RESET%^\"",}),({1501515231,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"Ecause that says \"if 2 > level\" right? that's all that ever says?%^RESET%^\"",}),({1501515433,"%^BOLD%^RED%^You tell Mahkefel,%^RESET%^ \"Eh, I think so -- that shorthand switch shit always confuses me.%^RESET%^\"",}),({1501515458,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"Yeah i hate it.%^RESET%^\"",}),({1501515473,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"It's a great way to make something really simple into something really confusing *armcrosses*.%^RESET%^\"",}),({1501515785,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"I have no idea how this is working.%^RESET%^\"",}),({1501515799,"%^BOLD%^RED%^You tell An invisible immortal,%^RESET%^ \"Lol.%^RESET%^\"",}),({1501515826,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"Oh oh.%^RESET%^\"",}),({1501515835,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"Being blessed multiplies your level by 2, for resistance checks.%^RESET%^\"",}),({1501515973,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"So yeah... bless is pretty much fear resistance/immunity then?%^RESET%^\"",}),({1501516144,"%^BOLD%^RED%^You tell An invisible immortal,%^RESET%^ \"Kinda.%^RESET%^\"",}),({1501516175,"%^BOLD%^RED%^You tell An invisible immortal,%^RESET%^ \"I mean, that's like, one of the things it does? I didn't realize it was so good though. 2x is pretty good once you get to like level 100.%^RESET%^\"",}),({1501516187,"%^BOLD%^RED%^You tell An invisible immortal,%^RESET%^ \"Not that it helps with some of the retardo fear levels.%^RESET%^\"",}),({1501516275,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"That's like... stryke though. pretty much just stryke?%^RESET%^\"",}),({1501516281,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"Maybe tulu?%^RESET%^\"",}),({1501516290,"%^BOLD%^RED%^You tell An invisible immortal,%^RESET%^ \"Rook too I think.%^RESET%^\"",}),({1501516322,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"Ah, i've forgotten how to find rook ^^.%^RESET%^\"",}),({1501516359,"%^BOLD%^RED%^You tell An invisible immortal,%^RESET%^ \"It's not as hard as it used to be? Just do a grid search for 'the different thing' that isn't the statue in westwood and like climb down I think.%^RESET%^\"",}),({1501517047,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"Yeah i got nothing.%^RESET%^\"",}),({1501517062,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"Also: man i love huge virtuals ^^.%^RESET%^\"",}),({1503516342,"%^BOLD%^RED%^You tell Mahkefel,%^RESET%^ \"Https://gist.github.com/sabosan/1d4a8e215b270ab9c6a1bfb3e28d0949 <-- recap of what I said.%^RESET%^\"",}),({1503946246,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"I HATE HER JOB.%^RESET%^\"",}),({1503946274,"%^BOLD%^RED%^You tell Mahkefel,%^RESET%^ \"I really have no idea why, it seems like a great place. They really value people.%^RESET%^\"",}),({1513701696,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"One day.%^RESET%^\"",}),({1513701705,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"I'm just going to increase exp gain 10-fold.%^RESET%^\"",}),({1513701710,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"And tell nobody.%^RESET%^\"",}),({1513701729,"%^BOLD%^RED%^You tell Mahkefel,%^RESET%^ \"Lol.%^RESET%^\"",}),({1513701750,"%^BOLD%^RED%^You tell Mahkefel,%^RESET%^ \"One of these days.... boom! Pow! Straight to the moon!%^RESET%^\"",}),({1513701757,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"Yes!%^RESET%^\"",}),({1513701768,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"I'll leave it divided by 10 for bjorns so eric never notices.%^RESET%^\"",}),({1513701862,"%^BOLD%^RED%^You tell Mahkefel,%^RESET%^ \"Obviously.%^RESET%^\"",}),({1527614894,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"Fucking duuk math.%^RESET%^\"",}),({1527614926,"%^BOLD%^RED%^Mahkefel tells you,%^RESET%^ \"Two skills * 0.4 isn't 80% of one skill if you add 5 other things in there before multiplying by 0.4.%^RESET%^\"",}),})
